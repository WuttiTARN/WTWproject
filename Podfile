# Uncomment the next line to define a global platform for your project
 platform :ios, '8.0'

target 'WTW' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WTW
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'

  target 'WTWTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WTWUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
